int led = 11; // on introduit la variable

void setup() {
  // put your setup code here, to run once:
  pinMode (led, OUTPUT); // on déclare la LED en sortie 
  
}

void loop() { // début de la boucle
  // put your main code here, to run repeatedly:
  analogWrite (led, 64); // la LED s'allume à 25% de son intensité
  delay(1000); // attente de 1 seconde
  analogWrite (led, 191); // la led s'allume à 75% de son intensité
  delay(1000);
  analogWrite (led, 255); // la LED s'allume à 100% de son intensité
  delay(1000);

}
